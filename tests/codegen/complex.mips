    lw $t0 0($sp)
    lw $t0 0($t0)
    lw $t0 12($t0)
    jal $t0
    addiu $sp $sp 4
    # end execute method reflect_0


    lw $t1 0($sp)
    addiu $sp $sp 4
    lw $t0 4($t0)
