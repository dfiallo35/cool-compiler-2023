# Text section
.text
main:
    jal Main_class
    addiu $sp $sp -4
    sw $v0 0($sp)
    jal Main_main
    j exit
# Create class Main
Main_class:
    li $a0 4
    li $v0 9
    syscall 
    la $t0 Main
    sw $t0 0($v0)
    addiu $v0 $v0 4
    addiu $sp $sp -4
    sw $ra 0($sp)
    lw $ra 0($sp)
    addiu $sp $sp 4
    addiu $v0 $v0 -4
    jr $ra


# Create class Complex
Complex_class:
    li $a0 12
    li $v0 9
    syscall 
    la $t0 Complex
    sw $t0 0($v0)
    addiu $v0 $v0 4
    addiu $sp $sp -4
    sw $ra 0($sp)
    addiu $sp $sp -4
    sw $v0 0($sp)
    # attribute x: Int
    addiu $sp $sp -4
    sw $v0 0($sp)
    li $a0 8
    li $v0 9
    syscall 
    la $t0 Int
    sw $t0 0($v0)
    li $t0 0
    sw $t0 4($v0)
    move $t0 $v0
    addiu $sp $sp 4
    lw $v0 0($sp)
    sw $t0 0($v0)
    addiu $v0 $v0 4
    # end attribute x: Int


    lw $v0 0($sp)
    addiu $sp $sp 4
    sw $t0 0($v0)
    addiu $v0 $v0 4
    addiu $sp $sp -4
    sw $v0 0($sp)
