    move $t0 $v0
    addiu $sp $sp 4
    lw $t0 4($t0)
    la $t1 true
    beq $t1 $t0 then_0
    # execute class method out_string
    addiu $sp $sp -8
    li $a0 8
    li $v0 9
    syscall 
    la $t0 String
    sw $t0 0($v0)
