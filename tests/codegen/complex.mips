    lw $v0 0($sp)
    addiu $sp $sp 4
    sw $t0 0($v0)
    addiu $v0 $v0 4
    lw $ra 0($sp)
    addiu $sp $sp 4
    addiu $v0 $v0 -12
    jr $ra


# Create function main from class Main
Main_main:
    addiu $sp $sp -4
