    sw $t0 8($sp)
    lw $t0 0($sp)
    lw $t0 0($t0)
    lw $t0 4($t0)
    jal $t0
    addiu $sp $sp 12
    # end execute method init


    sw $t0 0($sp)
    # if_0
    # execute method reflect_Y
    addiu $sp $sp -4
    # execute method reflect_X
    addiu $sp $sp -4
    # get variable c
    lw $t0 8($sp)
    # end get variable c
    sw $t0 0($sp)
    lw $t0 0($sp)
    lw $t0 0($t0)
    lw $t0 16($t0)
    jal $t0
    addiu $sp $sp 4
    # end execute method reflect_X


    sw $t0 0($sp)
    lw $t0 0($sp)
    lw $t0 0($t0)
    lw $t0 20($t0)
    jal $t0
    addiu $sp $sp 4
    # end execute method reflect_Y


    addiu $sp $sp -4
