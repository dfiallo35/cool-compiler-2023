# Function to set bool
set_bool:
    lw $t0 0($sp)
    addiu $sp $sp 4
    lb $t1 true
    beq $t0 $t1 set_bool_true
    la $t0 false
    jr $ra
  set_bool_true:
    la $t0 true
    jr $ra


# Function to get string length
String_length:
    lw $t0 4($sp)
    lw $t0 4($t0)
    addi $t1 $zero -1
String_length_loop:
    lb $t2 0($t0)
    addi $t0 $t0 1
    addi $t1 $t1 1
    bnez $t2 String_length_loop
    move $t0 $t1
    li $a0 8
    li $v0 9
    syscall 
    la $t1 Int
    sw $t1 0($v0)
    sw $t0 4($v0)
    move $t0 $t1
    jr $ra


# Function to concat strings
String_concat:
    li $t3 0
    addiu $sp $sp -4
    sw $ra 0($sp)
    lw $t0 4($sp)
    addiu $sp $sp -4
    sw $t0 0($sp)
    jal String_length
    addiu $sp $sp 4
    add $t3 $zero $t0
    lw $t0 8($sp)
    addiu $sp $sp -4
    sw $t0 0($sp)
    jal String_length
    addiu $sp $sp 4
    add $t3 $t3 $t0
    addiu $t3 $t3 1
    lw $ra 0($sp)
    addiu $sp $sp 4
    move $a0 $t3
    li $v0 9
    syscall 
    move $t3 $v0
    lw $t1 0($sp)
    lw $t1 4($t1)
    lw $t0 4($sp)
    lw $t0 4($t0)
    li $t2 0
String_concat_string1:
    lb $t2 0($t0)
    beq $t2 $0 String_concat_string2
